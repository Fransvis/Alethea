<%- include ("../partials/header") %>

<div class="container newForm">
	
	<h1 class="mx-sm-3">Create New Resource</h1>
	
		<form action="/resourceIndex" method="POST">
			<div class="form-group mx-sm-3">
				<label>Title</label>
				<input class="form-control" type="text" name="title" placeholder="title">
			</div>
			<div class="form-group mx-sm-3">
				<label>Subtitle</label>
				<input class="form-control" type="text" name="subtitle" placeholder="subtitle">
			</div>
			<div class="form-group mx-sm-3">
				<label>link</label>
				<input class="form-control" type="text" name="link" placeholder="link">
			</div>
			<div class="form-group mx-sm-3">
				<label>image</label>
				<input class="form-control" type="text" name="image" placeholder="image">
			</div>
			<div class="form-group mx-sm-3">
				<label>Author</label>
				<input class="form-control" type="text" name="author" placeholder="Author">
			</div>
			<button class="btn btn-outline-info mx-sm-3" href="#">Submit</button>
		</form>
	<a href="/resourceIndex">Back</a>
</div>

<%- include ("../partials/footer") %>